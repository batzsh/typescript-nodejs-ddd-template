{
  "name": "ts-node-clean-arch-ddd-template",
  "version": "0.3.0",
  "main": "index.js",
  "author": "Henrique Balmant <henrique.balmant@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/application/server.ts",
    "test": "jest --runInBand",
    "lint": "eslint ./src/**/*.*",
    "prepare": "husky install",
    "commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "swagger-ui-express": "^4.4.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
